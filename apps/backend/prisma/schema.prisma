datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:postgres@localhost:5432/mydatabase"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  repoId    Int
}
